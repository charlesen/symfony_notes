<form method="post" action="{{ path('note_delete', {'id': note.id}) }}" onsubmit="return confirm('Supprimer la note ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ note.id) }}">
    <button class="btn">Suppr.</button>
</form>
